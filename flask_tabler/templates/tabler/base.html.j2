<!DOCTYPE html>
<html {% block html_attributes %}style="color-scheme: dark;" data-theme="dark"{% endblock %}>
{%- block html %}
<head >
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">

    <title>{% block title %}{{title|default}}{% endblock title %}</title>


    <!-- CSS IMPORTS
    <link rel="stylesheet" type="text/css" href="" />
    -->
    {% block styles -%}
        
    <link rel="stylesheet" href="{{url_for('static', filename='tabler/css/' + tabler_version + '/tabler.min.css')}}">
    <style>
    @font-face {
        font-family: "Inter";
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url("{{url_for('static', filename='tabler/fonts/InterVariable.woff2')}}") format('woff2');
    }
    :root {
        --tblr-font-sans-serif: 'Inter';
    }
    </style>
    <link rel="shortcut icon" href="{{ url_for('static', filename='favicon.ico') }}">
    {% endblock -%}
</head>
<body data-bs-theme="dark">
    {%- block body %}
    <div class="{%block page_class %} page {% endblock %}">
        {%- block navbar -%}
        {%- endblock %}
        <div class="page-wrapper">
            {% block page_header %}
            <div class="container-xl">
                <div class="page-header d-print-none">
                    {% with messages = get_flashed_messages(with_categories=true) %}
                    {% if messages %}
                    {% for category, message in messages %}
                        <div class="alert alert-{{ category }}">{{ message }}</div>
                    {% endfor %}
                    {% endif %}
                    {% endwith %}
                </div>
            </div>
            {% endblock page_header %}  
            <div class="page-body">
                {%- block content -%}
                {%- endblock %}
            </div>
        </div>
    </div>
    {%- endblock %}

    <!-- JS IMPORTS
    <script type="application/javascript" src=""></script>
    -->
    <script type="application/javascript" src="{{url_for('static', filename='tabler/js/' + tabler_version + '/tabler.min.js')}}"></script>
    <script type="application/javascript" src="{{url_for('static', filename='tabler/js/imask.min.js')}}"></script>
    {% block scripts %}{% endblock %}
</body>
{% endblock %}
</html>

